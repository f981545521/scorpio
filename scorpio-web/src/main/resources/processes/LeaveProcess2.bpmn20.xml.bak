        <omgdi:waypoint x="515.0" y="24.999996185302734"></omgdi:waypoint>
        <omgdi:waypoint x="255.0" y="24.999996185302734"></omgdi:waypoint>
        <omgdi:waypoint x="255.0" y="60.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="bossPassFlow" id="BPMNEdge_bossPassFlow">
