        <omgdi:waypoint x="1096.0" y="100.0"></omgdi:waypoint>
        <omgdi:waypoint x="1096.0" y="165.00000000000003"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="directorNotPassFlow" id="BPMNEdge_directorNotPassFlow">
